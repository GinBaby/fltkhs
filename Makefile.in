DIRS = src examples
.SILENT:
all:
	for dir in $(DIRS); do\
		echo "=== making $$dir ===";\
		(cd $$dir; $(MAKE) all) || exit 1;\
	done

clean:
	for dir in $(DIRS); do\
		echo "=== cleaning $$dir ===";\
		(cd $$dir; $(MAKE) clean) || exit 1;\
	done

